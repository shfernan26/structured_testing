launch_files:
  - package: structured_testing
    file: my_system_launch.launch

published_topics:
  - /associated_object
  - /filtered_obj
  - /tracked_obj
  - /target_output

required_topics: 
  - /Mobileye_CAN_Rx
  - /Front_Radar_CAN_Rx
  - /Ground_Truth

bag_files:
  - /sys_test_bag_1/sys_test_bag_1.db3

test_duration:
  - 30
  
observers:
  - name: SystemPrecisionRecall
    observerClass: PrecisionRecallObserver
    topic:
      - "/tracked_obj"
      - "/Ground_Truth"
    msgType:
      - "common.msg.TrackedOutputMsg"
      - "common.msg.GroundTruth"
    field:
      - "obj_dx"
      - "obj_dy"
      - "dx"
      - "dy"
    num_targets: 2
    output_type: fixed
    tol: 4
    precision_pass_score: 0.6
    recall_pass_score: 0.6
